<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>dotnet-passbook</id>
        <version>1.0.0-alpha</version>
        <title>Passbook Generator</title>
        <authors>Tomas McGuinness</authors>
        <owners />
        <licenseUrl>http://tomasmcguinness.mit-license.org/</licenseUrl>
        <projectUrl>https://github.com/tomasmcguinness/dotnet-passbook</projectUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <description>Generate pass files for Apple's iOS 6.0 to iOS 9.0 passbook/Wallet. This library allows you to build and sign a pkpass file.</description>
        <summary>Generate pass files for Apple's iOS 6.0 to iOS 9.0 passbook/Wallet.</summary>
        <releaseNotes>Added support for barcodes allowing you to add an array of barcodes to a pass. Added support for Code128 barcode only available in barcodes array.</releaseNotes>
        <tags>PassBook Wallet</tags>
        <dependencies>
            <group targetFramework=".NETFramework4.6.1">
                <dependency id="Newtonsoft.Json" version="8.0.3" />
            </group>
        </dependencies>
